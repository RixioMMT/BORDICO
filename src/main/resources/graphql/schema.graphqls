schema {
    query: Query
    mutation: Mutation
}

type Query {
    addUser(userInput: UserInput!): User
}

type Mutation {
    addUser(userInput: UserInput!): User
}

type User {
    id: String!
    email: String!
    phone: String
    password: String!
    firstName: String
    lastName: String
    dateOfBirth: String
}

input UserInput {
    email: String!
    phone: String
    password: String!
    firstName: String
    lastName: String
    dateOfBirth: String
}
